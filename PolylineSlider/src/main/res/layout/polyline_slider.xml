<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

        <olil3.polylineSlider.uiComponents.YAxis
            android:id="@+id/mYAxis"
            android:layout_width="35dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/space"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">
        </olil3.polylineSlider.uiComponents.YAxis>

        <FrameLayout
            android:id="@+id/polyline_slider_graph_frame_layout"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_above="@id/polyline_x_axis_frame_layout"
            android:overScrollMode="never"
            app:layout_constraintBottom_toTopOf="@+id/polyline_x_axis_frame_layout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/mYAxis"
            app:layout_constraintTop_toTopOf="parent" />

        <FrameLayout
            android:id="@+id/polyline_x_axis_frame_layout"
            android:layout_width="0dp"
            android:layout_height="25dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/space"/>

        <Space
            android:id="@+id/space"
            android:layout_width="35dp"
            android:layout_height="25dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
